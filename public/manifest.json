{
	"name": "Tab Volumes",
	"description": "Adjust chrome tab volumes",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "Adjust volumes"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage", "activeTab", "scripting", "tabs"],
	"host_permissions": ["http://*/", "https://*/"],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	}
}
